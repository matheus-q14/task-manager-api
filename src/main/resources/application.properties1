spring.application.name=task-manager
# datasource PostgreSQL
spring.datasource.url=jdbc:postgresql://localhost:5432/task_manager_db
spring.datasource.username=linuxdb
spring.datasource.password=dbsql
# jpa
spring.jpa.database-plataform=org.hibernate.dialect.PostgreSQLDialect
spring.jpa.hibernate.ddl-auto=update
spring.jpa.properties.hibernate.show_sql=true
spring.jpa.properties.hibernate.format_sql=true
